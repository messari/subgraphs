{
  "env": {
    "es2021": true
  },
  "extends": ["airbnb/base", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "quotes": ["error", "double"],
    "import/extensions": "off",
    "no-restricted-syntax": ["off", "ForOfStatement"],
    "no-console": "off"
  }
}
