{
  "name": "curve",
  "license": "UNLICENSED",
  "scripts": {
    "prettier": "prettier --config ./.prettierrc.yaml --write \"**/*.{js,json,md,ts}\"",
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy": "graph deploy --node https://api.thegraph.com/deploy/ tannishmango/curve",
    "create-local": "graph create --node http://localhost:8020/ tannishmango/curve",
    "remove-local": "graph remove --node http://localhost:8020/ tannishmango/curve",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 tannishmango/curve",
    "prepare:mainnet": "mustache config/mainnet.json subgraph.template.yaml > subgraph.yaml"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.27.0",
    "@graphprotocol/graph-ts": "0.24.1",
    "mustache": "^4.2.0"
  }
}
