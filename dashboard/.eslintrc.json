{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": [
            "tsconfig.json"
        ],
        "warnOnUnsupportedTypeScriptVersion": false
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "emitWarning": true
    }
}